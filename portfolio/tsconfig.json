{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["dom", "es2015"],
      "jsx": "react",
      "moduleResolution": "node",
      "module": "ESNext",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      
    }
}
  